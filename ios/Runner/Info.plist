	<string>Flutter geliştirme araçları, yerel ağ üzerinden uygulamayla iletişim kurmak için bu izne ihtiyaç duyar.</string>
	<key>NSBonjourServices</key>
	<array>
		<string>_dartobservatory._tcp</string>
	</array>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
